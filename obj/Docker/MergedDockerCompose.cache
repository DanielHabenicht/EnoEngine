services:
  enolauncher:
    build:
      context: C:\Users\Benni\repositories\EnoEngine
      dockerfile: EnoLauncher/Dockerfile
    image: enolauncher
  enopostgres:
    environment:
      POSTGRES_PASSWORD: docker
      POSTGRES_USER: docker
    image: postgres
    ports:
    - 127.0.0.1:5432:5432/tcp
    volumes:
    - enowarspostgres:/var/lib/postgresql:rw
version: '3.4'
volumes:
  enowarspostgres: {}